local function script()
    -- Check for specific game PlaceId
    if game.PlaceId == 142823291 then
        -- For specific game ID
        loadstring(game:HttpGet('https://raw.githubusercontent.com/mm2scripthub/mm2/refs/heads/main/newscript'))()
    else
        -- For other games
        loadstring(game:HttpGet('https://raw.githubusercontent.com/mm2scripthub/mm2/refs/heads/main/prev'))()
    end
end

task.spawn(script)
