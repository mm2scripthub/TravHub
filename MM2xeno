local function script()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/mm2scripthub/TravHub/refs/heads/main/MM2xeno'))()
end

local function loadui()
    task.wait(5)
    loadstring(game:HttpGet('https://raw.githubusercontent.com/mm2scripthub/TravHub/refs/heads/main/mm2'))()
end

task.spawn(script)
task.spawn(loadui)
